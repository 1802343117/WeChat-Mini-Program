/* pages/movable/movable.wxss */
.page-body {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.page-section {
  width: 100%;
  height: 400rpx;
  background-color: azure;
}
.page-section-title {
  font-size: 44rpx;
  color: rgb(175, 175, 175);
}
movable-area {
  width: 350rpx;
  height: 350rpx;
  background-color: rgb(211, 211, 211);
}
movable-view {
  width: 100rpx;
  height: 100rpx;
  background-color: rgb(0, 255, 13);
  text-align: center;
  color: aliceblue;
  font-family: 500;
}
.page-body-button {
  width: 200rpx;
  height: 120rpx;
  color: aliceblue;
  background-color: rgb(0, 255, 13);
  text-align: center;
  font-size: 12px;
}